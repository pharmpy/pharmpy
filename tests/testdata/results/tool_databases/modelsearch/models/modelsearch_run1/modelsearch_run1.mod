$PROBLEM ABSORPTION(ZO)
$INPUT ID VISI XAT2=DROP DGRP DOSE FLAG=DROP ONO=DROP
       XIME=DROP NEUY SCR AGE SEX NYHA=DROP WT DROP ACE
       DIG DIU NUMB=DROP TAD TIME VIDD=DROP CLCR AMT SS II DROP
       CMT=DROP CONO=DROP DV EVID=DROP OVID=DROP RATE
$DATA ../.datasets/modelsearch_run1.csv IGNORE=@
$SUBROUTINES ADVAN1 TRANS2
$ABBR REPLACE ETA_MDT=ETA(4)
$PK
MDT = THETA(4)*EXP(ETA_MDT)
CL = THETA(1) * EXP(ETA(1))
VC = THETA(2) * EXP(ETA(2))
MAT = THETA(3) * EXP(ETA(3))
V = VC
ALAG1 = MDT
D1 = 2*MAT
$ERROR
CONC = A(1)/VC
Y = CONC + CONC * EPS(1)
$ESTIMATION METHOD=0
$THETA (0, 24.2927, Inf) ; POP_CL
$THETA (0, 109.163, Inf) ; POP_VC
$THETA (0, 0.260324, Inf) ; POP_MAT
$THETA  (0,0.155413) ; POP_MDT
$OMEGA 0.470976; IIV_CL
$OMEGA 0.59527; IIV_VC
$OMEGA 0.312688; IIV_MAT
$OMEGA  0.01 ; IIV_MDT
$SIGMA 0.19422; RUV_PROP
$TABLE ID TIME DV CWRES CIPREDI NOAPPEND FILE=mytab_mox1
