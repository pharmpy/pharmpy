$PROBLEM    PHENOBARB SIMPLE MODEL
$DATA      ../../pheno.dta IGNORE=@
$INPUT      ID TIME AMT WGT APGR DV
$SUBROUTINE ADVAN1 TRANS2
$PK
CL=THETA(1)*EXP(ETA(1))
V=THETA(2)*EXP(ETA(2))
S1=V

;;;SCM-LINEARIZE_CONSTANTS
$ERROR
Y=F+F*EPS(1)

D_EPSETA1_1 = 0
D_EPSETA1_2 = 0
"LAST
"  D_EPSETA1_1=HH(1,2)
"  D_EPSETA1_2=HH(1,3)
$THETA  (0,0.00581756) ; TVCL
$THETA  (0,1.44555) ; TVV
$OMEGA  0.111053  ;       IVCL
$OMEGA  0.201526  ;        IVV
$SIGMA  0.0164177
$ESTIMATION METHOD=1 INTERACTION MCETA=1 MAXEVALS=0
$TABLE      ID DV MDV CIPREDI=OPRED H011 TIME AMT WGT APGR G011 G021
            ETA1 ETA2 D_EPSETA1_1 D_EPSETA1_2 NOPRINT NOAPPEND
            ONEHEADER FILE=pheno_linbase.dta
            RFORMAT="(1PE16.9,300(1PE24.16))"
